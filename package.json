{
  "name": "vue3-admin-template",
  "version": "1.9.3",
  "license": "MIT",
  "author": "kuanghua",
  "packageManager": "pnpm@7.9.0",
  "scripts": {
    "dev": "vite --mode serve-dev",
    "test": "vite --mode serve-test",
    "build:test": "vite build --mode  build-test",
    "build": "vite build --mode build",
    "preview:build": "npm run build && vite preview ",
    "preview": "vite preview ",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx src --fix",
    "prepare": "husky install",
    "test:unit": "vue-cli-service test:unit",
    "test:watchAll": "vue-cli-service test:unit --watchAll",
    "test:cov": "vue-cli-service test:unit --coverage",
    "test:majestic": "majestic",
    "vitest": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "axios": "1.0.0",
    "echarts": "5.3.2",
    "element-plus": "^2.2.17",
    "js-error-collection": "^1.0.8",
    "mitt": "3.0.0",
    "moment-mini": "2.22.1",
    "nprogress": "0.2.0",
    "path": "0.12.7",
    "path-to-regexp": "6.2.0",
    "pinia": "^2.0.22",
    "vue": "3.2.37",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@types/echarts": "4.9.7",
    "@types/mockjs": "1.0.6",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "@vitejs/plugin-legacy": "1.6.4",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue-jsx": "1.3.1",
    "@vitest/coverage-c8": "^0.22.1",
    "@vitest/ui": "^0.22.1",
    "@vue/cli-plugin-unit-jest": "4.5.17",
    "@vue/cli-service": "4.5.17",
    "@vue/test-utils": "^2.1.0",
    "@vueuse/core": "^8.9.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-define-config": "1.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.1.1",
    "husky": "7.0.2",
    "jsdom": "16.4.0",
    "majestic": "1.8.1",
    "mockjs": "1.1.0",
    "prettier": "2.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.55.0",
    "svg-sprite-loader": "6.0.11",
    "typescript": "^4.8.4",
    "unocss": "^0.33.5",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.19.9",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^3.1.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mkcert": "^1.10.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "1.2.1",
    "vite-plugin-svg-icons": "1.0.5",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.34.17"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "html-webpack-plugin",
        "vite-plugin-mock",
        "unplugin-auto-import",
        "unplugin-vue-components",
        "vue-template-compiler",
        "unocss",
        "unplugin",
        "vite-plugin-mock",
        "@vitejs/plugin-legacy",
        "@vitejs/plugin-vue",
        "@vitejs/*",
        "@babel/*",
        "vite",
        "vue",
        "@unocss/vite",
        "rollup",
        "vue-jest",
        "@babel/*"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">= 16 <18",
    "pnpm": ">= 6 <8"
  }
}
